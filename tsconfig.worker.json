{
	"compilerOptions": {
		"target": "ESNext",
		"lib": [
			"ESNext"
		],
		"module": "ESNext",
		"moduleResolution": "bundler",
		"customConditions": [
			"workerd",
			"worker",
			"browser"
		],
		"types": [
			"@cloudflare/workers-types",
			"vite/client",
			"jest"
		],
		"paths": {
			"shared/*": [
				"./shared/*"
			],
			"worker/*": [
				"./worker/*"
			]
		},
		"composite": true,
		"tsBuildInfoFile": "./node_modules/.tmp/tsconfig.worker.tsbuildinfo",
		"skipLibCheck": true,
		"strict": true,
		"allowImportingTsExtensions": true,
		"noEmit": true
	},
	"include": [
		"./worker-configuration.d.ts",
		"./worker/env.d.ts",
		"./shared",
		"./worker"
	]
}